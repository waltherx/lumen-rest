{
    "name": "typingmaster/api-server",
    "description": "TypingMaster API server",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "nordsoftware/lumen-oauth2": "dev-master",
        "nordsoftware/lumen-cors": "dev-master",
        "codegyre/robo": "dev-master",
        "league/flysystem-aws-s3-v2": "^1.0@dev",
        "alchemy/zippy": "^0.2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
